{
  "apiVersion":"2.0.0-data-ingest-seurat-rds-automated",
  "experimentId": "e52b39624588791a7889e39c617f669e",
  "experimentName": "Multisample-murine-gamma-delta-T-wt-beta2-KO-lungs",
  "can_write": ["admin"],
  "meta": {
    "organism": "mmusculus",
    "type": "10x"
  },
  "processingConfig": {
    "cellSizeDistribution": {
      "enabled": true,
      "filterSettings": {
        "minCellSize": 100,
        "binStep": 200
      }
    },
   "dataIntegration": {
      "enabled": true,
       "dataIntegration": {
           "method": "seuratv4",
           "methodSettings": {
               "seuratv4": {
                   "numGenes": 2000,
                   "normalisation": "logNormalize"
               }
           }
       },
       "dimensionalityReduction": {
           "method": "rpca", 
           "numPCs": 30,
           "excludeGeneCategories": []
       }
   },
    "mitochondrialContent": {
      "enabled": true,
      "filterSettings": {
        "method": "absolute_threshold",
        "methodSettings": {
          "absolute_threshold": {
            "maxFraction": 0.1,
            "binStep": 0.05
          }
        }
      }
    },
    "readAlignment": {
      "enabled": true,
      "filterSettings": {
        "method": "absolute_threshold",
        "methodSettings": {
          "absolute_threshold": {
            "filterThreshold": 0.5,
            "bandwidth": -1
          }
        }
      }
    },
    "classifier": {
      "enabled": true,
      "auto": true,
      "filterSettings": {
        "FDR": 0.1
      }
    },
    "numGenesVsNumUmis": {
      "enabled": true,
      "auto": true,
      "filterSettings": {
        "regressionType": "gam",
        "regressionTypeSettings": {
          "gam": {
            "p.level": 9.090083e-05
          }
        }
      }
    },
    "doubletScores": {
      "enabled": true,
      "filterSettings": {
        "probabilityThreshold": 0.2,
        "binStep": 0.05
      }
    },
    "dimensionalityReduction": {
      "method": "pca",
      "methodSettings": {
        "pca": {
          "maxPCs": 10
        }
      },
      "interpolate": "linear",
      "excludeGeneCategories": {
        "ribosomal": true,
        "mitochondrial": true,
        "cellCycle": true
      }
    },
    "configureEmbedding": {
      "embeddingSettings": {
        "method": "umap",
        "methodSettings": {
          "umap": {
            "minimumDistance": 0.1,
            "distanceMetric": "euclidean"
          },
          "tsne": {
            "perplexity": 30,
            "learningRate": 200
          }
        }
      },
      "clusteringSettings": {
        "method": "louvain",
        "methodSettings": {
          "louvain": {
            "resolution": 0.5
          }
        }
      }
    }
  }
}